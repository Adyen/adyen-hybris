 ;en;"Englisch"
 ;ja;"Japanisch"
 ;zh;"Chinesisch"

# Currencies
UPDATE Currency;isocode[unique=true];name[lang=$lang]
 ;EUR;"Euro"
 ;GBP;"Pound"
 ;JPY;"Japanese Yen"
 ;USD;"US Dollar"

# Titles
UPDATE Title;code[unique=true];name[lang=$lang]
 ;dr;"Dr."
 ;miss;"Fr√§ulein"
 ;mr;"Herr"
 ;mrs;"Frau"
