		</parameter>
		<queryString>
			<![CDATA[select CAST(DAY as DATE) as DAY, avg(TOTPRICE) as TOTALPRICE, avg(TOTQUANTITY) as TOTALQUANTITY
from (
{{
select {oe.creationtime} as DAY, sum({oe.totalPrice} * {c2.conversion}/{c.conversion}) as TOTPRICE, sum({oe.quantity}) as TOTQUANTITY
from {OrderEntry as oe JOIN Order as o ON {oe.order}={o.PK}  JOIN Currency as c ON {o.currency}={c.PK} JOIN Currency as c2 ON {c2.pk}= $P{Currency}
LEFT OUTER JOIN CMSSite as s ON {o.site}={s.PK}}
where {oe.creationtime} >= to_date($P{From}) AND {oe.creationtime} < to_date($P{To}) AND ($P{Site} IS NULL OR {s.pk}=$P{Site})
group by {oe.order}, {oe.creationtime}
}})
group by DAY]]>
		</queryString>
