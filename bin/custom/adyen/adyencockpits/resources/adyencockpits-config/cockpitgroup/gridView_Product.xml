<?xml version="1.0" encoding="UTF-8"?>
<grid-view xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="grid-view.xsd">
	<imageslot>
		<property qualifier="Product.thumbnail.URL"/>
	</imageslot>
	<labelslot>
		<property qualifier="Product.name" />
	</labelslot>
	<descriptionslot>
		<property qualifier="Product.code" prefix="# ID" />
	</descriptionslot>
	<shortinfoslot>
		<property qualifier="Product.catalogVersion.mnemonic"/>
	</shortinfoslot>
	<actionslot spring-bean="ProductContentBrowserActionColumn"/>
	<specialactionslot spring-bean="ProductContentBrowserStatusColumn"/>

</grid-view>
